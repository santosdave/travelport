<?xml version="1.0" encoding="UTF-8"?>

<definitions name="UtilService"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://www.travelport.com/service/util_v49_0"
             xmlns:ns1="http://www.travelport.com/schema/util_v49_0"
             targetNamespace="http://www.travelport.com/service/util_v49_0">

    <import namespace="http://www.travelport.com/service/util_v49_0"
            location="UtilAbstract.wsdl"/>

    <binding name="UtilCreditCardAuthBinding" type="tns:UtilCreditCardAuthPortType">
        <soap:binding style="document"
                      transport="http://schemas.xmlsoap.org/soap/http" />
        <operation name="service">
            <soap:operation soapAction="http://localhost:8080/kestrel/CreditCardAuthorizationService"/>
            <input>
                <soap:body use="literal" />
            </input>
            <output>
                <soap:body use="literal" />
            </output>
            <fault name="ErrorInfoMsg">
                <soap:fault name="ErrorInfoMsg" use="literal" />
            </fault>
        </operation>
    </binding><service name="UtilCreditCardAuthService"><port name="UtilCreditCardAuthPort" binding="tns:UtilCreditCardAuthBinding">
            <soap:address
                    location="http://localhost:8080/kestrel/CreditCardAuthorizationService" />
        </port></service></definitions>